{
    "name": "yaroslawww/nova-naviga-ad-preview",
    "description": "Laravel nova NavigaAd api requests preview.",
    "keywords": [
        "nova",
        "naviga-ad"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yaroslav Georgitsa",
            "email": "yg@thinkone.group",
            "homepage": "https://think.studio/",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0",
        "yaroslawww/laravel-naviga-ad": "^1.0|^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0",
        "psalm/plugin-laravel": "^2.8",
        "vimeo/psalm": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "NovaNavigaAdPreview\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaNavigaAdPreview\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaNavigaAdPreview\\ToolServiceProvider"
            ]
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --colors=always --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
